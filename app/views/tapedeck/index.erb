<img id="cassette"
     src="data:image/png;base64,"
     onclick="window.showEmail()"></img>
<% form_for @user, :url => download_path  do |form| %>
  <%= form.text_field :email, :size => 40, :class => "hidden" %>
<% end %>
<div id="bottom-buttons">
  <a href="https://github.com/jhurwich/Tapedeck">
    <img id="github" src="data:image/png;base64,">
  </a>
  <a href="https://github.com/jhurwich/Tapedeck/issues">
    <img id="reportbug" src="data:image/png;base64,">
  </a>
</div>

<script type="text/javascript">
  function showEmail() {
    var elem = document.getElementById("user_email");
    elem.className = elem.className.replace("hidden", "");
  }
  var emailInput = document.getElementById("user_email");
  if (emailInput.value != "") {
    showEmail();
  }
  else {
    console.log("NO VALUE");
  }
  addTextHint(emailInput, "Email");
</script>
